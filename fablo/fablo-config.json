{
  "$schema": "https://github.com/hyperledger-labs/fablo/releases/download/1.2.0/schema.json",
  "global": {
    "fabricVersion": "2.4.7",
    "tls": false,
    "engine": "docker",
    "peerDevMode": false
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "domain": "orderer.example.com"
      },
      "orderers": [
        {
          "groupName": "group1",
          "type": "solo",
          "instances": 1
        }
      ]
    },
    {
      "organization": {
        "name": "Org1",
        "domain": "org1.example.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    },
    {
      "organization": {
        "name": "Org2",
        "domain": "org2.example.com"
      },
      "peer": {
        "instances": 1,
        "db": "CouchDb"
      }
    }
  ],
  "channels": [
    {
      "name": "register-channel",
      "orgs": [
        {
          "name": "Org1",
          "peers": ["peer0"]
        },
        {
          "name": "Org2",
          "peers": ["peer0"]
        }
      ]
    },
    {
      "name": "entrust-channel",
      "orgs" :[
        {
          "name": "Org1",
          "peers": ["peer0"]
        },
        {
          "name": "Org2",
          "peers": ["peer0"]
        }
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "EstateRegister",
      "version": "0.0.1",
      "lang": "node",
      "channel": "register-channel",
      "directory": "./chaincodes/chaincode-EstateRegister-node",
      "endorsement": "OR('Org1MSP.member', 'Org2MSP.member')"
    },
    {
      "name": "LeaseRegister",
      "version": "0.0.1",
      "lang": "node",
      "channel": "register-channel",
      "directory": "./chaincodes/chaincode-LeaseRegister-node",
      "endorsement": "OR('Org1MSP.member', 'Org2MSP.member')"
    },
    {
      "name": "EstateAgent",
      "version": "0.0.1",
      "lang": "node",
      "channel": "entrust-channel",
      "directory": "./chaincodes/chaincode-EstateAgent-node",
      "endorsement": "OR('Org1MSP.member', 'Org2MSP.member')"
    }
  ]
}
